    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-visualizer": "^5.7.1",
    "tailwindcss": "^3.1.6",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-html-env": "^1.2.3",
    "vite-plugin-svgr": "^2.2.1"
